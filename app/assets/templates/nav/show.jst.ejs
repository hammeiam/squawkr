<ul>
	<li class="left">
		<span><a href="/#"><h1 class="logo">squawkr</h1></a></span>
	</li>
	<% if(!!currentUser){ %>
	<li class="right">
		<span>
			<a href="javascript:void(0)" id="signOutAction">Sign Out</a>
		</span>
	</li>
	<li class="right">
		<span>
			<!-- <a href="javascript:void(0)" id="newPostAction">Create New Post</a> -->
			<a href="#newPostModal" data-toggle="modal" id="newPostAction">Create New Post</a>
		</span>
	</li>
	<li class="right">
		<a href="#u/<%= currentUser.escape('username') %>">
			<img src="<%= currentUser.escape('image_url') %>">
		</a>
	</li>
	<% } else{ %>
	<li class="right"><a href="/auth/twitter">Sign In</a></li>
	<% }; %>
</ul>